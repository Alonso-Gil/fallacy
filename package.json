{
  "name": "fallacy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf packages/**/node_modules && rm -rf apps/**/node_modules && yarn clean:cache",
    "clean:cache": "rm -rf packages/**/.turbo && rm -rf apps/**/.turbo && rm -rf apps/**/.next",
    "compile": "turbo run compile",
    "check-types": "turbo run check-types",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "check-format": "turbo run check-format",
    "start:next": "yarn compile && cd apps/next && yarn start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@nextui-org/react": "^2.2.9",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/container-queries": "^0.1.1",
    "framer-motion": "^11.0.3",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "tailwind-merge": "^2.2.1",
    "yup": "^1.3.3",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^8.8.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "esbuild": "^0.17.17",
    "eslint": "^8.50.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rollup": "^2.77.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "turbo": "^1.10.12",
    "typescript": "^5"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "resolutions": {
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  }
}
